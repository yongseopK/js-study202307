{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "object-curly-spacing" : "off",
        "eol-last": "off",
        "linebreak-style": "off",
        "require-jsdoc" : "off"
    }
}
